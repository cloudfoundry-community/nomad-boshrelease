client {
  enabled = true
  servers = <%= p('nomad.lan') %>
  node_id = "<%= name %>/<%= index %>"
  <%# network_interface = "REPLACE_NETWORK_INTERFACE" %>
  options {
    docker.endpoint = "unix:///var/vcap/sys/run/docker/docker.sock"
    <%# consul.address = "<%= discover_external_ip %1>:8500" %>
    docker.cleanup.image = "false"
  }
}
